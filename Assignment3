//JAVA Chapter2
//Assignment 3


public class loop {

	public static void main(String[] args) {
		 for(int i=0;i<=4;i++){
		        for(int j=1;j<=i+1;j++){
		            System.out.print(j);
		        }
		        System.out.println();

		    }
		    for(int i=4;i>=1;i--){
		        for(int j=1;j<=i;j++){
		            System.out.print(j);
		        }
		        System.out.println();

		    }
	}
}
